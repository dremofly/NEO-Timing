<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.neotimingtest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NeoTimingTest"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity_afterSale"></activity>
        <activity android:name=".activity_propertyPage1" />
        <activity android:name=".activity_loadingPage" />
        <activity android:name=".activity_ExchangePage" />
        <activity android:name=".activity_instructionPage" />
        <activity android:name=".activity_ItemPage" />
        <activity android:name=".activity_workPage" />
        <activity android:name=".activity_timePicker" />
        <activity android:name=".activity_createWallet" />
        <activity android:name=".activity_importWallet" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>